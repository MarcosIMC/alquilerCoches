<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>alquilerCochesServlet</servlet-name>
  	<display-name>alquilerCochesServlet</display-name>
  	<description></description>
  	<servlet-class>alquilerCoches.alquilerCochesServlet</servlet-class>
  
	<init-param>
		<param-name>base</param-name>
		<param-value>/alquilerCoches/alquilerMoya</param-value>
	</init-param> 
  	
  </servlet>
  <servlet>
  	<servlet-name>registroUsuarioServlet</servlet-name>
  	<display-name>registroUsuarioServlet</display-name>
  	<description></description>
  	<servlet-class>alquilerCoches.registroUsuarioServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>alquilerCochesServlet</servlet-name>
  	<url-pattern>/alquilerMoya</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>registroUsuarioServlet</servlet-name>
  	<url-pattern>/registroUsuarioServlet</url-pattern>
  </servlet-mapping>
</web-app>
